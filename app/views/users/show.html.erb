<div class="col-xs-12">
  <%= attachment_image_tag(@user, :image, :fill, 50, 50, fallback: "no-image-mini.jpg") %>
  <%= @user.name %>
  <%= @user.introduction %>
  <% if @user == current_user %>
  <div class="row">
    <%= link_to 'プロフィール編集',edit_user_path(@user), class: "col-xs-3 btn btn-default edit_user_#{@user.id}" %>
    <%#= link_to '退会',_path(@user), class: "col-xs-3 btn btn-default edit_user_#{@user.id}" %>
  </div>
  <% end %>
</div>